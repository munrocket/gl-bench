{
  "name": "gl-bench",
  "version": "0.0.7",
  "description": "A proper WebGL performance benchmark that uses GPU timers inside",
  "main": "dist/gl-bench.js",
  "module": "dist/gl-bench.module.js",
  "minimized": "dist/gl-bench.min.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "npm run build && concurrently \"rollup -c -w\" \"npm:test\"",
    "ci": "concurrently -k -s first \"npm:puppeteer\" \"http-server -c-1 -p 1234\"",
    "puppeteer": "node --experimental-modules --es-module-specifier-resolution=node --no-warnings ./test/puppeteer.js",
    "deploy": "npm version patch && git push --tags origin master"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "0.10.0",
    "concurrently": "4.1.2",
    "http-server": "0.11.1",
    "puppeteer": "1.19.0",
    "rollup": "1.20.3",
    "zora": "3.0.2"
  },
  "author": "munrocket",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munrocket/gl-bench.git"
  },
  "keywords": [
    "webgl",
    "webgl2",
    "benchmark",
    "performance",
    "timer",
    "stats.js",
    "pixi",
    "three",
    "babylon",
    "regl",
    "cesium"
  ]
}
